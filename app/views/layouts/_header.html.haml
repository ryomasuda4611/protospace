.navbar.navbar-default.navbar-static-top.navbar-fixed-top
  .container
    %button.navbar-toggle(type="button" data-toggle="collapse" data-target=".navbar-responsive-collapse")
      %span.icon-bar
      %span.icon-bar
      %span.icon-bar
    %a.navbar-brand(href="/")
      = image_tag("logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "36px")
    .navbar-collapse.collapse.navbar-responsive-collapse
      //not login
      %ul.nav.navbar-nav.navbar-right
        %li
          -unless user_signed_in?
            .btn.btn-primary.navbar-btn
              = link_to "Get Started", new_user_session_path
        %li
          -if user_signed_in?
            .btn.btn-primary.navbar-btn
              =link_to "Log_out", destroy_user_session_path, :method=>:delete
      %ul.nav.navbar-nav.navbar-right
        %li.dropdown
          %a.dropdown-toggle{"aria-expanded" => "false", "data-toggle" => "dropdown", href: "#", role: "button"}
            UserName
            %span.caret
          %ul.dropdown-menu{role: "menu"}
            %li
              =link_to "Profile Edit", "#"
            %li
              =link_to "Logout", "#"
        %li
          .btn.btn-primary.navbar-btn
            =link_to "New Proto", "/proto/new"
